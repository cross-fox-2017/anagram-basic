<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <form class="" action="/" method="get">
      <label for="">Kata yang ingin dicari: </label>
      <input type="text" name="word">
      <input type="submit" value="submit">
    </form>
    <p>Anagram dari <strong><%= word %></strong> adalah</p>
    <% anagrams.forEach(function(value){
      <li><%= value %></li>
    <% }) %>
  </body>
</html>

extends layout

block content
  h1= title

  form(action='/', method='get')
    label Kata yang ingin dicari:
    input(type='text', name='word')
    input(type='submit', value='submit')

  p anagram dari kata #{word} adalah :
  if word
    ul
      each anagram in anagrams.length ? anagrams : ['There are no values']
        li= anagram
